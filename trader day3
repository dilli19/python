def maxProfit(prices):
    n = len(prices)
    if n < 2:
        return 0
    first_buy = -prices[0]
    second_buy = -prices[0]
    first_sell = 0
    second_sell = 0
    for i in range(1, n-1):
        first_buy = max(first_buy, -prices[i])
        first_sell = max(first_sell, first_buy + prices[i])
        second_buy = max(second_buy, first_sell - prices[i])
        second_sell = max(second_sell, second_buy + prices[i])
    return second_sell
prices = list(map(int, input("Enter the prices separated by commas: ").split(',')))
print("The maximum profit is:", maxProfit(prices))

